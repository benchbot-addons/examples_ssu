#!/usr/bin/env python3

# Demo agent for showing evaluation procedure. Multiple simple scenarios available.
# User gets to define what test is being done for what task

from benchbot_api import BenchBot
from eval_semantic_slam_agent import EvalSemanticSlamAgent

if __name__ == '__main__':
    print("NOTE: this example doesn't actually use the task, and will work in "
          "any task. Normally though, it would be important to run this with "
          "a semantic SLAM task. ")
    BenchBot(agent=EvalSemanticSlamAgent()).run()
